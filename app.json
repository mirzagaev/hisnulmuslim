    },
    "web": {
      "favicon": "./assets/images/icon.png"
    },
    "scheme": "hisnulmuslim",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "db66e2ea-564f-424f-b767-36e849da049a"
      }
    },
    "owner": "mirzagaev"
